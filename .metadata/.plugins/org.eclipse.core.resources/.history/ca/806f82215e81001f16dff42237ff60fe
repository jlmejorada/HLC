package boletin01.ej08;

public class Ejercicio08 {
	
	public static int fibonacci(int num) {
		int res = 0;
		
		
		if (num>0) {
			
		} else {
			throw new Exception("El n√∫mero tiene que ser positivo");
		}
		
		
		if (num==1 || num==2) {		
			
			res=1;
			
		} else {
			res=(fibonacci(num-1))+(fibonacci(num-2));
		}
		
		return res;
	}
}
